windows
.\main.exe create "/home/1.txt"
.\main.exe create "/home/2.txt"
.\main.exe create "//home/3.txt"
.\main.exe list "/home"


macos  or unix
./main create "/home/1.txt"
./main create "/home/2.txt"
./main create "/home/dwl/1.txt"
./main create "/home/dwl/2.txt"
./main list "/home"
./main list "/home/1.txt"
./main write "/home/1.txt" 0123456789012345678901234567890123456789012345678901234567890123
./main write "/home/1.txt" a b c d e
./main read "/home/1.txt"
./main append "/home/1.txt" a b c d e
./main append "/home/1.txt" 0123456789012345678901234567890123456789012345678901234567890123
./main delete "/home/1.txt"

